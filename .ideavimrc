set surround

nmap go :action MoveEditorToOppositeTabGroup<CR>
nmap gb :action Back<CR>
nmap gr :action MoveTabRight<CR>
nmap gt /^class.*TestCase<CR>W:action CopyReference<CR>
nmap <A-l> :action NextTab<CR>
nmap <A-h> :action PreviousTab<CR>

set ideajoin
set hlsearch
set incsearch
set scrolloff=6
